<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.system.mapper.SysRoleFunctionMapper">

    <select id="listRoleFunction" resultType="com.smart.system.model.SysRoleFunctionPO">
        SELECT A.* FROM sys_role_function A
        join sys_function B on A.function_id = B.function_id
        ${ew.customSqlSegment}
    </select>
</mapper>
